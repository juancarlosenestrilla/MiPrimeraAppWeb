<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    
    
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />


</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <!--Html.ActionLink("Nombre", "Accion", "Controlador", new { area = "" }, new { class = "navbar-brand" })-->
                @Html.ActionLink("Curso", "Curso", "Curso", new { area = "" }, new { @class = "navbar-brand" });
                @Html.ActionLink("Periodo", "Periodo", "Periodo", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Seccion", "Seccion", "Seccion", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Alumnos", "Alumnos", "Alumno", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Docente", "Docente", "Docente", new { area = "" }, new { @class = "navbar-brand" })
                @Html.ActionLink("Repaso", "Ini", "Repaso", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                </ul>
            </div>
        </div>
    </div>

    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

   
</body>
</html>